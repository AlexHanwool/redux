{
  "gitbook": "3.x.x",
  "title": "Redux",
  "structure": {
    "readme": "README_kr.md",
    "summary": "docs_kr/README.md"
  },
  "plugins": ["edit-link", "prism", "-highlight", "github", "anchorjs"],
  "language": "ko",
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/dobbit/redux/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/reactjs/redux/"
    },
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "build/gitbook.css"
      }
    }
  }
}

